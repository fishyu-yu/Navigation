{
  "name": "navigation-system",
  "version": "1.0.0",
  "description": "A modern navigation website system with card-based layout, theme switching, user authentication and website status monitoring",
  "main": "server.js",
  "keywords": [
    "navigation",
    "website",
    "dashboard",
    "bookmark",
    "status-monitor",
    "express",
    "nodejs",
    "postgresql"
  ],
  "author": "Navigation System Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishyu-yu/Navigation.git"
  },
  "bugs": {
    "url": "https://github.com/fishyu-yu/Navigation/issues"
  },
  "homepage": "https://github.com/fishyu-yu/Navigation#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup.js",
    "init": "node scripts/init-wizard.js",
    "init-silent": "node scripts/init-wizard.js --silent",
    "init-db": "psql -U postgres -d navigation -f scripts/init-db.sql",
    "seed": "node scripts/seed-database.js",
    "sample-data": "node scripts/add-sample-data.js",
    "clear-data": "node scripts/clear-all-data.js",
    "clear-db-sql": "psql -f scripts/clear-database.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.17.3",
    "node-fetch": "^2.7.0",
    "pg": "^8.12.0",
    "pg-mem": "^3.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
